<van-tabbar active="{{ active }}" active-color="#009fad"
bind:change="onChange">
  <van-tabbar-item wx:for="{{list}}" wx:key="index">
    <image slot="icon" src="{{ item.iconPath }}" mode="aspectFit" style="width: 65rpx; height: 48rpx;" />
    <image slot="icon-active" src="{{ item.selectedIconPath }}" mode="aspectFit" style="width: 65rpx; height: 48rpx;" />
    {{item.text}}
  </van-tabbar-item>
</van-tabbar>
